APP_NAME="BoostClicks CPA Platform"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://openai-book.store

APP_LOCALE=ru
APP_FALLBACK_LOCALE=ru
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

SEED_DEFAULT_PASSWORD=ChangeMe123!
SEED_SUPER_ADMIN_PASSWORD=${SEED_DEFAULT_PASSWORD}
SEED_SUPER_ADMIN_EMAIL=owner@platform.test

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=info

DB_CONNECTION=pgsql
DB_HOST=127.0.0.1
DB_PORT=5432
DB_DATABASE=cpa_db
DB_USERNAME=cpa_user
DB_PASSWORD=change_me_db_password
DB_SCHEMA=public
DB_SSLMODE=prefer
DB_PGSQL_TIMEOUT=5

SESSION_DRIVER=database
SESSION_LIFETIME=10080
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=noreply@boostclicks.ru
MAIL_PASSWORD=change_me_mail_password
MAIL_FROM_ADDRESS="noreply@boostclicks.ru"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

GEOIP_ENABLED=true
GEOIP_PREFER_DETECTED_GEO=false
GEOIP_ATTACH_DETECTED_GEO_TO_EXTRA_DATA=true
IP2LOCATION_DB_PATH=storage/app/ip2location/IP2LOCATION-LITE-DB3.BIN
IP2LOCATION_MODE=FILE_IO
GEOIP_AUTO_UPDATE_ENABLED=true
# Optional direct download URL. Leave empty to use official endpoint + token + package.
GEOIP_AUTO_UPDATE_URL=
GEOIP_AUTO_UPDATE_ENDPOINT=https://www.ip2location.com/download/
GEOIP_AUTO_UPDATE_TOKEN=
GEOIP_AUTO_UPDATE_PACKAGE=DB3LITEBINIPV6
GEOIP_AUTO_UPDATE_DAILY_AT=03:20
GEOIP_AUTO_UPDATE_CONNECT_TIMEOUT=10
GEOIP_AUTO_UPDATE_TIMEOUT=180

CLICKHOUSE_ENABLED=false
CLICKHOUSE_HOST=127.0.0.1
CLICKHOUSE_PORT=8123
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=
CLICKHOUSE_DATABASE=analytics
CLICKHOUSE_HTTPS=false
CLICKHOUSE_TIMEOUT=10
CLICKHOUSE_CONNECT_TIMEOUT=5
CLICKHOUSE_TABLE_LEAD_EVENTS=lead_events
CLICKHOUSE_RETENTION_MONTHS=24

VITE_APP_NAME="${APP_NAME}"
